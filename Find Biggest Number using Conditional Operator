//wap to find the biggest number in the given 2 numbers using conditional operator.//

#include<stdio.h>
int main()
{
    int x,y;
    printf("enter your number");
    scanf("%d%d",&x,&y);
    (x>y)?printf("%d is greater",x):printf("%d is greater",y);
}

